{
  "expo": {
    "name": "Lemontoy",
    "slug": "LEMONTOY",
    "version": "1.8.0",
    "orientation": "portrait",
    "icon": "./assets/images/app_icon_logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alphatechassist.lemontoy",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.alphatechassist.lemontoy",
      "usesCleartextTraffic": true,
      "versionCode": 6,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app_icon_logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": []
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/app_icon_logo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5e8a70c7-6a0f-4ad3-ab23-d94b2d6cff75"
      }
    }
  }
}
